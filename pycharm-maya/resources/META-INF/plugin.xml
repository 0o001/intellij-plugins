<idea-plugin version="2" xmlns:xi="http://www.w3.org/2001/XInclude">
  <name>Autodesk Maya Support</name>
  <id>org.jetbrains.plugins.maya</id>
  <description>Provides support for developing Python scripts for Maya</description>
  <vendor>JetBrains</vendor>
  <depends>com.intellij.modules.ultimate</depends>
  <depends>com.intellij.modules.python</depends>

  <xi:include href="/META-INF/pycharm-maya-plugin.xml" xpointer="xpointer(/idea-plugin/*)"/>

  <extensions defaultExtensionNs="com.intellij">
    <errorHandler implementation="com.intellij.diagnostic.ITNReporter"/>
  </extensions>
</idea-plugin>
