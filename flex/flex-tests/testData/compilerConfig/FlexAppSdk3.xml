<flex-config xmlns="http://www.adobe.com/2006/flex-config">
  <compiler>
    <warn-no-constructor>false</warn-no-constructor>
    <accessible>false</accessible>
    <fonts>
      <managers>
        <manager-class>flash.fonts.JREFontManager</manager-class>
        <manager-class>flash.fonts.AFEFontManager</manager-class>
        <manager-class>flash.fonts.BatikFontManager</manager-class>
      </managers>
    </fonts>
    <source-path>
      <path-element>$MODULE_DIR$/src</path-element>
    </source-path>
    <debug>true</debug>
    <locale>
      <locale-element>en_US</locale-element>
    </locale>
    <namespaces>
      <namespace>
        <uri>http://www.adobe.com/2006/mxml</uri>
        <manifest>$FLEX_SDK$/frameworks/mxml-manifest.xml</manifest>
      </namespace>
    </namespaces>
    <library-path>
      <path-element>$FLEX_SDK$/frameworks/locale/{locale}</path-element>
      <path-element>$FLEX_SDK$/frameworks/libs/framework.swc</path-element>
      <path-element>$FLEX_SDK$/frameworks/libs/rpc.swc</path-element>
      <path-element>$FLEX_SDK$/frameworks/libs/utilities.swc</path-element>
    </library-path>
    <external-library-path>
      <path-element>$FLEX_SDK$/frameworks/libs/player/9/playerglobal.swc</path-element>
    </external-library-path>
  </compiler>
  <target-player>9</target-player>
  <static-link-runtime-shared-libraries>false</static-link-runtime-shared-libraries>
  <output>output/folder/Unnamed.swf</output>
</flex-config>