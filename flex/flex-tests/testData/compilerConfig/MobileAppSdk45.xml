<flex-config xmlns="http://www.adobe.com/2006/flex-config">
  <compiler>
    <warn-no-constructor>false</warn-no-constructor>
    <mobile>true</mobile>
    <preloader>spark.preloaders.SplashScreen</preloader>
    <accessible>false</accessible>
    <fonts>
      <managers>
        <manager-class>flash.fonts.JREFontManager</manager-class>
        <manager-class>flash.fonts.BatikFontManager</manager-class>
        <manager-class>flash.fonts.AFEFontManager</manager-class>
        <manager-class>flash.fonts.CFFFontManager</manager-class>
      </managers>
    </fonts>
    <source-path>
      <path-element>$MODULE_DIR$/src</path-element>
    </source-path>
    <debug>true</debug>
    <locale>
      <locale-element>en_US</locale-element>
    </locale>
    <theme>
      <filename>$FLEX_SDK$/frameworks/themes/Mobile/mobile.swc</filename>
    </theme>
    <namespaces>
      <namespace>
        <uri>http://ns.adobe.com/mxml/2009</uri>
        <manifest>$FLEX_SDK$/frameworks/mxml-2009-manifest.xml</manifest>
      </namespace>
      <namespace>
        <uri>library://ns.adobe.com/flex/spark</uri>
        <manifest>$FLEX_SDK$/frameworks/spark-manifest.xml</manifest>
      </namespace>
    </namespaces>
    <library-path>
      <path-element>$FLEX_SDK$/frameworks/locale/{locale}</path-element>
      <path-element>$FLEX_SDK$/frameworks/libs/air/servicemonitor.swc</path-element>
      <path-element>$FLEX_SDK$/frameworks/libs/authoringsupport.swc</path-element>
      <path-element>$FLEX_SDK$/frameworks/libs/charts.swc</path-element>
      <path-element>$FLEX_SDK$/frameworks/libs/flash-integration.swc</path-element>
      <path-element>$FLEX_SDK$/frameworks/libs/framework.swc</path-element>
      <path-element>$FLEX_SDK$/frameworks/libs/osmf.swc</path-element>
      <path-element>$FLEX_SDK$/frameworks/libs/rpc.swc</path-element>
      <path-element>$FLEX_SDK$/frameworks/libs/spark.swc</path-element>
      <path-element>$FLEX_SDK$/frameworks/libs/textLayout.swc</path-element>
      <path-element>$FLEX_SDK$/frameworks/libs/mobile/mobilecomponents.swc</path-element>
    </library-path>
    <external-library-path>
      <path-element>$FLEX_SDK$/frameworks/libs/air/airglobal.swc</path-element>
    </external-library-path>
  </compiler>
  <target-player />
  <swf-version>11</swf-version>
  <static-link-runtime-shared-libraries>false</static-link-runtime-shared-libraries>
  <output>output/folder/Unnamed.swf</output>
</flex-config>