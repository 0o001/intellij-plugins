<?xml version="1.0"?>
<mx:Application
        xmlns:mx="http://www.adobe.com/2006/mxml">
    <mx:Script>
            new Button();
            new <error>Panel</error>();
            new <error>List</error>();
    </mx:Script>
    <mx:Button/>
    <mx:DataGrid>
        <mx:itemRenderer>
            <mx:Component>
                <mx:VBox>
                    <mx:Script>
                      new <error>Button</error>();
                      new Panel();
                      new <error>List</error><caret>();
                    </mx:Script>
                    <mx:Panel/>
                    <mx:DataGrid>
                        <mx:itemRenderer>
                            <mx:Component>
                                <mx:VBox>
                                    <mx:Script>
                                      new <error>Button</error>();
                                      new <error>Panel</error>();
                                      new List();
                                    </mx:Script>
                                    <mx:List/>
                                </mx:VBox>
                            </mx:Component>
                        </mx:itemRenderer>
                    </mx:DataGrid>
                </mx:VBox>
            </mx:Component>
        </mx:itemRenderer>
    </mx:DataGrid>
</mx:Application>
