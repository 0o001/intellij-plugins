mvn com.intellij.flex.maven:idea-flexmojos-maven-plugin:1.5.1:generate -o
mvn com.intellij.flex.maven:idea-flexmojos-maven-plugin:1.5.1:generate -DgenerateNonShareable=false -DgenerateShareable -Prelease -o
mvn com.intellij.flex.maven:idea-flexmojos-maven-plugin:1.5.1:generate -DgenerateNonShareable=false -DgenerateShareable -PreleaseForTesters -o

mvn com.intellij.flex.maven:idea-flexmojos-maven-plugin:1.5.1:generate -o -s build/maven-settings.xml

mvn com.intellij.flex.maven:idea-flexmojos-maven-plugin:1.5.1:replicate-repo -o -e


mvn compile -Prelease -Dflex.swf.uncompressed=true -o
mvn compile -PreleaseForTesters -Dflex.swf.uncompressed=true -o


diskutil erasevolume HFS+ "ramdisk" `hdiutil attach -nomount ram://2097152`
ln -s /Volumes/ramdisk/production $DEV_IDEA_HOME/out/classes/production