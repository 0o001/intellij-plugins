<flex-config xmlns="http://www.adobe.com/2006/flex-config">
	<compiler>
		<accessible>false</accessible>
		<debug>true</debug>
		<external-library-path append="true">
			<path-element>@@repo@@/com/adobe/flex/framework/airglobal/3.1.0.4880/airglobal.swc</path-element>
		</external-library-path>
		<fixed-literal-vector>true</fixed-literal-vector>
		<fonts>
			<local-fonts-snapshot>@@repo@@/fonts.ser</local-fonts-snapshot>
		</fonts>
		<locale/>
		<source-path append="true">
			<path-element>@@baseDir@@/src</path-element>
		</source-path>
		<theme/>
		<warn-no-constructor>false</warn-no-constructor>
	</compiler>
	<include-sources append="true">
		<path-element>@@baseDir@@/src</path-element>
	</include-sources>
	<metadata>
		<title>shared</title>
	</metadata>
	<output>@@target@@/shared-1.0-SNAPSHOT.swc</output>
	<swf-version>11</swf-version>
	<tools-locale>en_US</tools-locale>
</flex-config>