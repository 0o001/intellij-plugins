<idea-plugin>

  <actions>
    <group id="JstdGenerateCodeInsideGroup">
      <action id="QUnitGenerateNewTestAction" class="com.google.jstestdriver.idea.assertFramework.qunit.generate.QUnitGenerateNewTestAction"/>
      <action id="QUnitGenerateSetupAction" class="com.google.jstestdriver.idea.assertFramework.qunit.generate.QUnitGenerateSetupAction"/>
      <action id="QUnitGenerateTearDownAction" class="com.google.jstestdriver.idea.assertFramework.qunit.generate.QUnitGenerateTearDownAction"/>
      <action id="JstdCreateTestAction" class="com.google.jstestdriver.idea.assertFramework.jstd.generate.JstdGenerateNewTestAction"/>
      <action id="JstdCreateSetupAction" class="com.google.jstestdriver.idea.assertFramework.jstd.generate.JstdGenerateSetupAction"/>
      <action id="JstdCreateTearDownAction" class="com.google.jstestdriver.idea.assertFramework.jstd.generate.JstdGenerateTearDownAction"/>
      <add-to-group group-id="GenerateGroup" anchor="first"/>
    </group>
  </actions>

  <extensions defaultExtensionNs="com.intellij">
    <inspectionToolProvider implementation="com.google.jstestdriver.idea.assertFramework.support.JstdInspectionToolProvider"/>
    <postStartupActivity implementation="com.google.jstestdriver.idea.javascript.JSUnitTestingSupport"/>
  </extensions>

</idea-plugin>
