<idea-plugin version="2">
  <actions>
    <action id="Dart.pub.install" class="com.jetbrains.lang.dart.ide.actions.DartPubInstallAction"
            text="Dart pub install" description="Install Dart packages">
      <add-to-group group-id="CutCopyPasteGroup" anchor="last"/>
    </action>
    <action id="Dart.pub.update" class="com.jetbrains.lang.dart.ide.actions.DartPubUpdateAction"
            text="Dart pub update" description="Update Dart packages">
      <add-to-group group-id="CutCopyPasteGroup" anchor="last"/>
    </action>
  </actions>
</idea-plugin>
