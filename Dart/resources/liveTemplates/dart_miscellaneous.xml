<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="Dart">
  <template name="sv" value="static var $END$" description="static var" toReformat="false" toShortenFQNames="false">
    <context>
      <option name="DART" value="true"/>
    </context>
  </template>

  <template name="fs" value="$RETURN_TYPE$ $NAME$($ARGS$) => $END$ ;" description="simple function"
            toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;name&quot;" alwaysStopAt="true"/>
    <variable name="ARGS" expression="" defaultValue="" alwaysStopAt="true"/>
    <variable name="RETURN_TYPE" expression="" defaultValue="" alwaysStopAt="true"/>
    <context>
      <option name="DART" value="true"/>
    </context>
  </template>

  <template name="fb" value="$RETURN_TYPE$ $NAME$($ARGS$) {&#10; $END$ &#10;}" description="function with body"
            toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;name&quot;" alwaysStopAt="true"/>
    <variable name="ARGS" expression="" defaultValue="" alwaysStopAt="true"/>
    <variable name="RETURN_TYPE" expression="" defaultValue="" alwaysStopAt="true"/>
    <context>
      <option name="DART" value="true"/>
    </context>
  </template>

  <template name="sfs" value="static $RETURN_TYPE$ $NAME$($ARGS$) => $END$ ;" description="static simple function"
            toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;name&quot;" alwaysStopAt="true"/>
    <variable name="ARGS" expression="" defaultValue="" alwaysStopAt="true"/>
    <variable name="RETURN_TYPE" expression="" defaultValue="" alwaysStopAt="true"/>
    <context>
      <option name="DART" value="true"/>
    </context>
  </template>

  <template name="sfb" value="static $RETURN_TYPE$ $NAME$($ARGS$) {&#10; $END$ &#10;}" description="static function with body"
            toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;name&quot;" alwaysStopAt="true"/>
    <variable name="ARGS" expression="" defaultValue="" alwaysStopAt="true"/>
    <variable name="RETURN_TYPE" expression="" defaultValue="" alwaysStopAt="true"/>
    <context>
      <option name="DART" value="true"/>
    </context>
  </template>

  <template name="sout" value="print('$END$');" description="print to standard out"
            toReformat="true" toShortenFQNames="true">
    <context>
      <option name="DART" value="true"/>
    </context>
  </template>

  <template name="soutm" value="print('$CLASS_NAME$.$METHOD_NAME$');&#10;$END$" description="prints the current class and method name to standard out"
            toReformat="true" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="" defaultValue="dartClassName" alwaysStopAt="false"/>
    <variable name="METHOD_NAME" expression="" defaultValue="dartMethodName" alwaysStopAt="false"/>
    <context>
      <option name="DART" value="true"/>
    </context>
  </template>
</templateSet>

