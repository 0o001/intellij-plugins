<idea-plugin version="2">
  <id>org.jetbrains.plugin.flutter</id>
  <name>Flutter</name>
  <version>999.999</version>
  <vendor>JetBrains</vendor>

  <depends>Dart</depends>

  <description><![CDATA[
      Flutter-specific extensions for Dart.
    ]]></description>


  <extensions defaultExtensionNs="com.intellij">
    <configurationType implementation="com.jetbrains.lang.dart.ide.runner.flutter.FlutterRunConfigurationType"/>
    <runConfigurationProducer implementation="com.jetbrains.lang.dart.ide.runner.flutter.FlutterRunConfigurationProducer"/>
    <programRunner implementation="com.jetbrains.lang.dart.ide.runner.flutter.FlutterRunner"/>
  </extensions>

</idea-plugin>